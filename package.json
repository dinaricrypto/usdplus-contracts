{
  "name": "@dinari/usdplus-contracts",
  "version": "0.1.0",
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "metrics": "solidity-code-metrics ./src/**/*.sol --html > metrics.html",
    "errors": "npx ts-node script/errors_parse_ast.ts",
    "anvil:kinto": "anvil -f https://rpc.kinto-rpc.com/",
    "lint:json": "ajv validate -s releases/schema.json -d \"releases/v*/*.json\" --strict=true",
    "lint:json:debug": "echo \"Files to validate:\" && ls releases/v*/*.json && yarn lint:json"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "solidity-code-metrics": "^0.0.25",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ajv-cli": "^5.0.0"
  }
}
